<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core" >
   <List
        headerText="Lista de Vendas"
        growing="true"
		growingThreshold="4"
		growingScrollToLoad="false"
        class="sapUiResponsiveMargin"
        width="auto"
        items="{/VendaSet}" >
        <items>
            <StandardListItem
			title="{Descricao}"
			info="{ValorLiquido}"/>
      </items>
   </List>
</core:FragmentDefinition>