{
    "_version": "1.12.0",
    "sap.app": {
      "id": "cromos.it.walkthrough",
      "type": "application",
      "i18n": "i18n/i18n.properties",
      "title": "{{appTitle}}",
      "description": "{{appDescription}}",
      "applicationVersion": {
        "version": "1.0.0"
      },
      "dataSources": {
        "lojaRemote": {
          "uri": "https://inove-s4hana.opustech.com.br:5201/sap/opu/odata/sap/ZLOJA_SRV/",
          "type": "OData",
          "settings": {
          "odataVersion": "2.0"
          }
        }
      }  
    },
    "sap.ui": {
      "technology": "UI5",
      "deviceTypes": {
          "desktop": true,
          "tablet": true,
          "phone": true
      }
    },
    "sap.ui5": {
      "rootView": {
          "viewName": "cromos.it.walkthrough.view.Home",
          "type": "XML",
          "async": true,
          "id": "app"
      },
      "dependencies": {
        "minUI5Version": "1.60",
        "libs": {
          "sap.m": {}
        }
      },
      "models": {
        "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "cromos.it.walkthrough.i18n.i18n"
        }
        },
        "": {
          "dataSource": "lojaRemote"
          }
      }
    }
  }